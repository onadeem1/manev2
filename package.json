{
  "name": "manestream",
  "version": "1.0.0",
  "description": "manestream 2.0",
  "main": "index.js",
  "author": "Omer Nadeem <omernadeem3@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint-fix": "npm run lint -- --fix",
    "precommit": "lint-staged # un-comment to enable",
    "prettify": "prettier --write \"**/*.{js,jsx,json,css,scss,md}\"",
    "seed": "node database/seed.js",
    "start": "node server/index.js",
    "start-watch": "nodemon server/index.js --watch server --watch database --watch package.json",
    "start-dev": "cross-env NODE_ENV=development npm run start-watch"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "npm run lint-fix",
      "git add"
    ]
  },
  "dependencies": {
    "@google/maps": "^0.5.5",
    "bluebird": "^3.5.1",
    "compression": "^1.7.3",
    "connect-session-sequelize": "^5.2.2",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "lodash": "^4.17.10",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "pg-native": "^3.0.0",
    "sequelize": "^5.0.0-beta.10"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-fullstack": "^5.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.18.2",
    "prettier": "1.11.1"
  }
}
